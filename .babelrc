{
  "presets": [
    ["@babel/env", {
      "targets": {
        "browsers": ["last 2 versions"]
      }
    }],
    "@babel/react"
  ],
  "plugins": [
    "idx",
    "react-hot-loader/babel",
    "dynamic-import-webpack",
    "@babel/proposal-class-properties",
    "@babel/plugin-proposal-export-default-from",
    ["module-resolver", {
      "alias": {
        "^/(.+)": "./src/\\1"
      }
    }]
  ]
}
